{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "env": [
        "REACT_APP_ENDPOINT",
        "SPOTIFY_API_ENDPOINT",
        "REACT_APP_SPOTIFY_API_ENDPOINT"
      ]
    },
    "preview": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
